{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "askedio/laravel5-profanity-filter": "^1.10",
        "aws/aws-sdk-php": "^3.130",
        "barryvdh/laravel-dompdf": "^0.8.5",
        "barryvdh/laravel-translation-manager": "^0.5.7",
        "cohensive/embed": "5.5.*",
        "components/jquery": "^3.4",
        "ecrmnn/laravel-https": "^1.0",
        "fideloper/proxy": "^4.2",
        "imanghafoori/laravel-anypass": "^1.0",
        "imanghafoori/laravel-heyman": "^2.2",
        "imanghafoori/laravel-masterpass": "^2.0",
        "imanghafoori/laravel-widgetize": "^1.9",
        "laracademy/generators": "^2.1",
        "laravel/framework": "^6.2",
        "laravel/socialite": "^4.3",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^1.1",
        "laravelcollective/html": "^6.0",
        "laraveles/spanish": "^1.4",
        "league/flysystem-aws-s3-v3": "^1.0",
        "nesbot/carbon": "^2.28",
        "onecentlin/laravel-adminer": "^4.7",
        "pragmarx/countries": "^0.7.0",
        "snapappointments/bootstrap-select": "^1.13",
        "spatie/laravel-permission": "^3.3",
        "spatie/laravel-translatable": "^4.2",
        "twbs/bootstrap": "^4.4",
        "zanysoft/laravel-zip": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "beyondcode/laravel-dump-server": "^1.3",
        "facade/ignition": "^1.4",
        "filp/whoops": "^2.5",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0",
        "predis/predis": "^1.1",
		"snipe/banbuilder": "dev-master"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
